{
  "name": "quarantine-cup",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "divjoy"
  ],
  "dependencies": {
    "@analytics/google-analytics": "0.2.2",
    "@analytics/google-tag-manager": "^0.2.3",
    "@zeit/next-sass": "0.2.1-canary.4",
    "analytics": "0.3.0",
    "bootstrap": "4.4.1",
    "fake-auth": "0.1.0",
    "firebase": "7.6.2",
    "firebase-admin": "8.9.2",
    "mailchimp-api-v3": "1.13.1",
    "next": "9.2.2",
    "node-sass": "4.13.0",
    "query-string": "6.9.0",
    "react": "16.12.0",
    "react-bootstrap": "1.0.0-beta.16",
    "react-dom": "16.12.0",
    "react-hook-form": "4.10.1",
    "react-query": "1.0.29"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npm run build-decrypt && next build",
    "start": "next start",
    "build-encrypt": "./build/encrypt.sh",
    "build-decrypt": "./build/decrypt.sh",
    "encrypt": "openssl enc -aes-256-cbc -salt -in ./quarantine-cup-service-acc.json -out ./quarantine-cup-service-acc.enc -k",
    "decrypt": "openssl enc -aes-256-cbc -d -in ./quarantine-cup-service-acc.enc -out ./quarantine-cup-service-acc.json -k"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
